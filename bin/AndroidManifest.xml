<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kspku"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.kspku.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kspku.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.kspku.Pengguna"
            android:label="@string/title_activity_pengguna" >
        </activity>
        <activity
            android:name="com.example.kspku.Anggota"
            android:label="@string/title_activity_anggota" >
        </activity>
        <activity
            android:name="com.example.kspku.Peminjaman"
            android:label="@string/title_activity_peminjaman" >
        </activity>
        <activity
            android:name="com.example.kspku.CariAnggota"
            android:label="@string/title_activity_cari_anggota" >
        </activity>
        <activity
            android:name="com.example.kspku.BayarAngsuran"
            android:label="@string/title_activity_bayar_angsuran" >
        </activity>
    </application>

</manifest>